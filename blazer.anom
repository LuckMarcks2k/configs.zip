[SCRIPT]
REQUEST PUT "https://blaze.com/api/auth/password" 
  CONTENT "{\"username\":\"<USER>\",\"password\":\"<PASS>\"}" 
  CONTENTTYPE "application/json; charset=utf-8" 
  HEADER ": scheme: https" 
  HEADER "accept: application/json, text/plain, */*" 
  HEADER "accept-language: pt-BR,pt;q=0.9,en-US;q=0.8,en;q=0.7" 
  HEADER "content-length: 64" 
  HEADER "content-type: application/json;charset=UTF-8" 
  HEADER "cookie: _ga=GA1.2.2028289077.1652461307; _gid=GA1.2.261537999.1652461307; __zlcmid=19xkvFY6WRYJTw1" 
  HEADER "origin: https://blaze.com" 
  HEADER "referer: https://blaze.com/pt/?modal=auth&tab=login" 
  HEADER "sec-ch-ua: \" Not A;Brand\";v=\"99\", \"Chromium\";v=\"99\", \"Opera GX\";v=\"85\"" 
  HEADER "sec-ch-ua-mobile: ?0" 
  HEADER "sec-ch-ua-platform: \"Windows\"" 
  HEADER "sec-fetch-dest: empty" 
  HEADER "sec-fetch-mode: cors" 
  HEADER "sec-fetch-site: same-origin" 
  HEADER "x-client-language: pt" 
  HEADER "x-client-version: 8958dd8a" 

PARSE "<SOURCE>" JSON "access_token" -> VAR "TOKEN" 

REQUEST GET "https://blaze.com/api/wallets" 
  
  HEADER ": scheme: https" 
  HEADER "accept: application/json, text/plain, */*" 
  HEADER "accept-language: pt-BR,pt;q=0.9,en-US;q=0.8,en;q=0.7" 
  HEADER "authorization: Bearer <TOKEN>" 
  HEADER "cookie: _ga=GA1.2.2028289077.1652461307; _gid=GA1.2.261537999.1652461307; __zlcmid=19xkvFY6WRYJTw1; _gat=1" 
  HEADER "referer: https://blaze.com/pt/?menu=account" 
  HEADER "sec-ch-ua: \" Not A;Brand\";v=\"99\", \"Chromium\";v=\"99\", \"Opera GX\";v=\"85\"" 
  HEADER "sec-ch-ua-mobile: ?0" 
  HEADER "sec-ch-ua-platform: \"Windows\"" 
  HEADER "sec-fetch-dest: empty" 
  HEADER "sec-fetch-mode: cors" 
  HEADER "sec-fetch-site: same-origin" 
  HEADER "x-client-language: pt" 
  HEADER "x-client-version: 8958dd8a" 

PARSE "<SOURCE>" JSON "balance" CreateEmpty=FALSE -> CAP "Saldo" 

PARSE "<SOURCE>" JSON "bonus_balance" CreateEmpty=FALSE -> CAP "Bonus:" 

PARSE "<SOURCE>" JSON "real_balance" CreateEmpty=FALSE -> CAP "Real:" 

REQUEST GET "https://blaze.com/api/users/me" 
  
  HEADER ": scheme: https" 
  HEADER "accept: application/json, text/plain, */*" 
  HEADER "accept-language: pt-BR,pt;q=0.9,en-US;q=0.8,en;q=0.7" 
